<?xml version="1.0" encoding="iso-8859-1" ?>

<table>
  <base>CRM/Irasdonation</base>
  <class>IrasResponseLog</class>
  <name>civicrm_o8_iras_response_log</name>
  <comment>iras log response logs</comment>
  <log>true</log>

  <field>
    <name>id</name>
    <type>int unsigned</type>
    <required>true</required>
    <comment>Unique ID</comment>
  </field>
  <primaryKey>
    <name>id</name>
    <autoincrement>true</autoincrement>
  </primaryKey>

  <field>
    <name>response_body</name>
    <type>text</type>
    <required>true</required>
    <comment>json response of request</comment>
    <html>
      <type>ResponseBody</type>
    </html>
  </field>

  <field>
    <name>response_code</name>
    <type>int</type>
    <required>false</required>
    <comment>response code</comment>
    <html>
      <type>ResponseCode</type>
    </html>
  </field>  

  <field>
    <name>created_date</name>
    <type>datetime</type>
    <comment>Created date</comment>
    <html>
      <type>CreatedDate</type>
    </html>
  </field>

</table>
