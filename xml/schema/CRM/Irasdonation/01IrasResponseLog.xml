<?xml version="1.0" encoding="iso-8859-1" ?>

<table>
  <base>CRM/Irasdonation</base>
  <class>IrasResponseLog</class>
  <name>civicrm_o8_iras_response_log</name>
  <comment>IRAS submissions response logs, keeps all submission values and responses</comment>
  <log>true</log>

  <field>
    <name>id</name>
    <type>int unsigned</type>
    <required>true</required>
    <comment>Unique ID</comment>
  </field>
  <primaryKey>
    <name>id</name>
    <autoincrement>true</autoincrement>
  </primaryKey>

  <field>
    <name>is_api</name>
    <type>boolean</type>
    <required>false</required>
    <default>false</default>
    <comment>api or offline report</comment>
  </field>

  <field>
    <name>validate_only</name>
    <type>boolean</type>
    <required>false</required>
    <default>true</default>
    <comment>validateOnly</comment>
  </field>

  <field>
    <name>basis_year</name>
    <type>varchar</type>
    <length>4</length>
    <required>false</required>
    <default>""</default>
    <comment>BasisYear</comment>
  </field>

  <field>
    <name>organisation_id_type</name>
    <type>varchar</type>
    <length>60</length>
    <required>false</required>
    <default>""</default>
    <comment>organisationIDType</comment>
  </field>

  <field>
    <name>organisation_id_no</name>
    <type>varchar</type>
    <length>60</length>
    <required>false</required>
    <default>""</default>
    <comment>organisationIDNo</comment>
  </field>

  <field>
    <name>organisation_name</name>
    <type>varchar</type>
    <length>60</length>
    <required>false</required>
    <default>""</default>
    <comment>organisationName</comment>
  </field>

  <field>
    <name>batch_indicator</name>
    <type>varchar</type>
    <length>4</length>
    <required>false</required>
    <default>"0"</default>
    <comment>batchIndicator</comment>
  </field>

  <field>
    <name>authorised_person_id_no</name>
    <type>varchar</type>
    <length>60</length>
    <required>false</required>
    <default>""</default>
    <comment>authorisedPersonIDNo</comment>
  </field>

  <field>
    <name>authorised_person_name</name>
    <type>varchar</type>
    <length>60</length>
    <required>false</required>
    <default>""</default>
    <comment>authorisedPersonName</comment>
  </field>

  <field>
    <name>authorised_person_designation</name>
    <type>varchar</type>
    <length>60</length>
    <required>false</required>
    <default>""</default>
    <comment>authorisedPersonDesignation</comment>
  </field>

  <field>
    <name>telephone</name>
    <type>varchar</type>
    <length>60</length>
    <required>false</required>
    <default>""</default>
    <comment>telephone</comment>
  </field>

  <field>
    <name>authorised_person_email</name>
    <type>varchar</type>
    <length>60</length>
    <required>false</required>
    <default>""</default>
    <comment>authorisedPersonEmail</comment>
  </field>

  <field>
    <name>num_of_records</name>
    <type>int unsigned</type>
    <required>false</required>
    <default>0</default>
    <comment>numOfRecords</comment>
  </field>

  <field>
    <name>total_donation_amount</name>
    <type>int unsigned</type>
    <required>false</required>
    <default>0</default>
    <comment>totalDonationAmount</comment>
  </field>

  <field>
    <name>response_body</name>
    <type>text</type>
    <required>false</required>
    <comment>json response of request</comment>
    <html>
      <type>TextArea</type>
    </html>
  </field>

  <field>
    <name>response_code</name>
    <type>int</type>
    <required>false</required>
    <default>NULL</default>
    <comment>response code</comment>
    <html>
      <type>Text</type>
    </html>
  </field>

  <field>
    <name>created_date</name>
    <title>Created Date</title>
    <type>timestamp</type>
    <default>CURRENT_TIMESTAMP</default>
    <comment>When the response was first received</comment>
  </field>
  <index>
    <name>index_created_date</name>
    <fieldName>created_date</fieldName>
  </index>

</table>
